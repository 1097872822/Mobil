page{
    background-color: #dedede;
    padding: 20rpx;
}
.search_row{
    height: 70rpx;
    display: flex;
}
input{
    flex: 1;
    background-color: #fff;
    padding-left: 30rpx;
}
button{
    width: 110rpx;
    height: 100%;
    padding: 0;
    margin: 0 10rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 26rpx;
}
.search_content{
    margin-top: 20rpx;
}
.search_item{
    background-color: #fff;
    font-size: 26rpx;
    padding: 15rpx 10rpx;
    border-bottom: 1rpx solid #ccc;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.floor_list_item_fir{
    text-align: center;
}
.ranking_list_im_fir{
    width: 200rpx;
    height: 250rpx;
    display: flex;
    float: left;
}
.rank_list_name_fir{
    font-size: 40rpx;
    font-weight: bolder;
}
.rank_list_brief{
    margin: 10rpx 0;
    font-size: 30rpx;
    color: dimgrey;
    display: -webkit-box;
    line-height: 70rpx;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:3;
    overflow: hidden;
    text-overflow:ellipsis;
}
.rank_list_writer{
    font-size: 25rpx;
    color: dimgrey;
    margin: 10rpx 0;
}
.boaddco{
    display: flex;
    margin-top: 20rpx;
}
.bo{
    flex: 1;
    color: orangered;
}
